{
  "extends": [
    "github>aquaproj/aqua-renovate-config#2.8.2",
    "config:best-practices"
  ],
  "packageRules": [
    {
      "groupName": "aqua devDependencies",
      "matchFileNames": [
        "aqua.ya?ml"
      ],
      "automerge": true
    },
    {
      "automerge": true,
      "matchPackageNames": [
